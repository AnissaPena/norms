def scalarVecMulti(scalar,vector):

result = []

for i in vector:

result.append(i * scalar)

return result


for i in range(len(vector01)):

total = vector01[i] - vector02[i]

result.append(total)

return result


def infNorm(vector):

norm = abs(vector[0])

for i in vector:

if abs(i) > norm:

norm = abs(i)

return norm


